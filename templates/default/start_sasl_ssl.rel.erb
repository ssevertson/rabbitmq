{release, {"<%= @erlang_otp %>","<%= @erlang_release %>"}, {erts, "<%= @erlang_version %>"},
 [
<% libraries = @erlang_library_versions.keys %>
<% last_library = libraries.pop %>
<% libraries.each do | library | -%>
